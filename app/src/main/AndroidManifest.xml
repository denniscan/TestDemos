<?xml version="1.0" encoding="utf-8"?>
<manifest package="dennis.testdemos"
	xmlns:android="http://schemas.android.com/apk/res/android">
	<application
		android:name=".utils.app.MyApp"
		android:allowBackup="true"
		android:icon="@mipmap/ic_launcher"
		android:label="@string/app_name"
		android:supportsRtl="true"
		android:theme="@style/AppTheme">
		<activity android:name=".ui.activity.MainActivity">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />

				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<activity android:name=".a01_materialdesign.ui.activity.MaterialDesignDemoActivity"
			android:theme="@style/AppTheme.NoActionBar" />
		<activity
			android:name=".a01_materialdesign.ui.activity.DetailActivity_CollapsingToolbar"
			android:theme="@style/AppTheme.NoActionBar" />
	</application>
</manifest>